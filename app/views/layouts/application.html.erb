  <script src="assets/javascripts/modernizr-2.8.3.min.js"></script>  <!-- Modernizr /-->


    <%= csrf_meta_tags %>

    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
